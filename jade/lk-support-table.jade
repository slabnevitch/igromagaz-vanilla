extends _misc/main

block title
	| Личный кабинет | Техподдержка

block content
	- var current = 9;
	.wrapper
		ul.breadcrumbs
			li.breadcrumbs__item 
				a(href="#").breadcrumbs__link Главная
			li.breadcrumbs__item 
				a(href="#").breadcrumbs__link Личный кабинет
			li.breadcrumbs__item 
				span.breadcrumbs__link Техподдержка

	.page.page-lk.page-lk-support.page-lk-support-table
		.wrapper-smart
			.wrapper-flex.page-lk__wrap
				include _blocks/lk-sidebar
				
				.page-lk__content
					.lk-min-header(data-da="page-lk__wrap,991,first")
						h1 Техподдержка
											
					.support-history		
						.support-history__title История обращений
						form.form(action="#").support-history__form
							label.form__label
								span.form__field-name Текст обращения:
								input(autocomplete="off" type="text" name="name" class="form__input")

							button.btn.btn-bg-green Найти

						.support-history__table		
							.lk-table
								.lk-table__header
									.lk-table__row
										.lk-table__col.lk-table__col--indicator Индикатор
										.lk-table__col.lk-table__col--id ID
										.lk-table__col.lk-table__col--title Заголовок
										.lk-table__col.lk-table__col--changed Изменено
										.lk-table__col.lk-table__col--author Кто изменил
										.lk-table__col.lk-table__col--messages Сообщения
										.lk-table__col.lk-table__col--status Статус
								
								.lk-history-table__body
									- for (var i=0; i < 5; i++)
										.lk-table__row
											.lk-table__col.lk-table__col--indicator
												div(class=i==0 ? "support-indicator support-indicator--green" : i==1 || i==3 ? "support-indicator support-indicator--red" : "support-indicator support-indicator--gray")
											.lk-table__col.lk-table__col--id
												.lk-table__desk 34656
												.lk-table__mob
													span ID
													| 34656
											.lk-table__col.lk-table__col--title Карта оплаты World of Warcraft – 60 дней RU (ключ для ПК)
											.lk-table__col.lk-table__col--changed
												.lk-table__desk
													p 17.07.2022 
													p 14:40:34
												.lk-table__mob
													p Изменено 17.07.2022 в 14:40:34
											
											.lk-table__col.lk-table__col--author elyy@indox.ru
											.lk-table__col.lk-table__col--messages
												.lk-table__desk 3
												.lk-table__mob
														| Сообщений:
														span 3
											.lk-table__col.lk-table__col--status
												.lk-table__desk Просмотрено
												.lk-table__mob
														| Статус:
														span Просмотрено

						.support-history__legend.legend-support-history
							.legend-support-history__item
								.support-indicator.support-indicator--green
								.legend-support-history__text Последний раз в обращение писал сотрудник техподдержки
							.legend-support-history__item
								.support-indicator.support-indicator--red
								.legend-support-history__text Последний раз в обращение писали вы
							.legend-support-history__item
								.support-indicator.support-indicator--gray
								.legend-support-history__text Обращение закрыто

						.pagination
							a(href="#").pagination__prev
								+icon('pagination-prev')
							ul.pagination-list
								li.pagination-list__item
									a(href="#").pagination-list__link 1
								li.pagination-list__item
									a(href="#").pagination-list__link 2
								li.pagination-list__item.active
									a(href="#").pagination-list__link 3
								li.pagination-list__item
									a(href="#").pagination-list__link 4
								li.pagination-list__item
									a(href="#").pagination-list__link ...
								li.pagination-list__item
									a(href="#").pagination-list__link 7
							
							a(href="#").pagination__next
								+icon('pagination-next')


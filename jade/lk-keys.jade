extends _misc/main


block title
	| Личный кабинет | Ключи и инструкции

block content
	- var current = 1;
	.wrapper
		ul.breadcrumbs
			li.breadcrumbs__item 
				a(href="#").breadcrumbs__link Главная
			li.breadcrumbs__item 
				a(href="#").breadcrumbs__link Личный кабинет
			li.breadcrumbs__item 
				span.breadcrumbs__link Ключи и инструкции

	.page.page-lk.page-lk-keys
		.wrapper-smart
			.wrapper-flex.page-lk__wrap
				include _blocks/lk-sidebar
				
				.page-lk__content
					.lk-min-header(data-da="page-lk__wrap,991,first")
						h1 Ключи и инструкции
	
					.lk-search-form
						form.form(action="#")
							.lk-search-form__row
								label.form__label
									span.form__field-name Номер заказа:
									input(autocomplete="off" type="text" name="name" class="form__input" placeholder="Введите номер")
								label.form__label
									span.form__field-name Заказ период
									input(autocomplete="off" type="text" name="name" class="form__input for-datepicker" placeholder="c")
								label.form__label
									input(autocomplete="off" type="text" name="name" class="form__input for-datepicker" placeholder="по" data-position="bottom right")

								label.form__label
									span.form__field-name Название товара:
									input(autocomplete="off" type="text" name="name" class="form__input" placeholder="Название")
							
							.lk-search-form__bottom
								button.btn.btn-bg-green Применить фильтр
								a(href="#").link Вернуть параметры по умолчанию

					.lk-table
						.lk-table__header
							.lk-table__row
								.lk-table__col.lk-table__col--order #заказа, дата
								.lk-table__col.lk-table__col--name Название
								.lk-table__col.lk-table__col--preview Просмотр
						
						.lk-history-table__body
							- for (var i=0; i < 8; i++)
								.lk-table__row
									.lk-table__col.lk-table__col--order №817409
										br
										| от 21.04.2022 16:32 
									
									.lk-table__col.lk-table__col--name
										a(href="#") Карта оплаты World of Warcraft – 60 дней RU (ключ для ПК)
									.lk-table__col.lk-table__col--preview
										p VoxreD : 6F4A3-NBRWL-DQH3X
										a(href="#").btn.btn-brd.btn-brd-accent.btn-sm Инструкция по активации

					.pagination
						a(href="#").pagination__prev
							+icon('pagination-prev')
						ul.pagination-list
							li.pagination-list__item
								a(href="#").pagination-list__link 1
							li.pagination-list__item
								a(href="#").pagination-list__link 2
							li.pagination-list__item.active
								a(href="#").pagination-list__link 3
							li.pagination-list__item
								a(href="#").pagination-list__link 4
							li.pagination-list__item
								a(href="#").pagination-list__link ...
							li.pagination-list__item
								a(href="#").pagination-list__link 7
						
						a(href="#").pagination__next
							+icon('pagination-next')

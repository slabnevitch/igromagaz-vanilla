extends _misc/main

block title
	| Личный кабинет | Техподдержка

block content
	- var current = 9;
	.wrapper
		ul.breadcrumbs
			li.breadcrumbs__item 
				a(href="#").breadcrumbs__link Главная
			li.breadcrumbs__item 
				a(href="#").breadcrumbs__link Личный кабинет
			li.breadcrumbs__item 
				span.breadcrumbs__link Техподдержка

	.page.page-lk.page-lk-support
		.wrapper-smart
			.wrapper-flex.page-lk__wrap
				include _blocks/lk-sidebar
				
				.page-lk__content
					.lk-address-form
						form.form(action="#")
							label.form__label
								span.form__field-name Заголовок:
								input(autocomplete="off" type="text" name="name" class="form__input")

								
							label.form__label
								span.form__field-name Сообщение:
								textarea(autocomplete="off" name="name" class="form__area")

							.form__files
								span.form__field-name Прикрепить файлы:
								p (Максимальный размер файла 1000 Кб)
								- for (var i=0; i < 2; i++)
									.form__filesrow
										label.file-lable
											input(type="file" name="mame" class="file-lable__file")
											span.file-lable__button Обзор
										.link Файл не выбран

							label.form__label
								span.form__field-name Критичность:
								span.lk-personal-form__select
										select(data-searchdisable="true" class="fstdropdown-select")
											// - var months = ['января','февраля','марта','апреля','мая','июня','июля','августа','сентября','октября','ноября','декабря'];
												// each item, ind in months
											- for (var i=0; i < 5; i++)
												option(value="0" + (ind+1)) Средняя

							label.form__label
								span.form__field-name Категория:
								span.lk-personal-form__select
										select(data-searchdisable="true" class="fstdropdown-select")
											- for (var i=0; i < 5; i++)
												option(value="0" + (ind+1)) Название категории
							
							button.btn.btn-bg-green Отправить сообщение						
